	<xs:simpleType name="tmcType">
		<xs:annotation>
			<xs:documentation>This is a Traffic Message Channel Code for a
				roadway
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{3}[\+\-NP]\d{5}" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="zipcode">
		<xs:annotation>
			<xs:documentation>This is a postal zip code</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{5}(-\d{4})?" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="emailType">
		<xs:annotation>
			<xs:documentation>This is an email address</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[\w.%-]+@[A-Za-z0-9.-]+\.[A-Za-z]{2,4}" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="phoneNumberType">
		<xs:annotation>
			<xs:documentation>This is a phone number in the format
				1-123-456-7890
				or 123-456-7890, possibly with an extension of 2-4 digits
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="(1\-)?\d{3}\-\d{3}\-\d{4}(x\d{2,4})?" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="lettersAndDigits">
		<xs:annotation>
			<xs:documentation>This is a string of uppercase and lowercase
				letters
				and digits
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[0-9A-Za-z]+" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="nonNegativeDecimal">
		<xs:annotation>
			<xs:documentation>This is a decimal greater than or equal to 0
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:minInclusive value="0" />
		</xs:restriction>
	</xs:simpleType>
