	<xs:annotation>
		<xs:documentation>
		  TMDD V3.0 Design - Manny Insignares, ConSysTec, May 29, 2006 
	   </xs:documentation>
	</xs:annotation>
	<!-- InformationalText -->
	<xs:simpleType name="InformationalText">
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="255"/>
		</xs:restriction>
	</xs:simpleType>
</xs:schema>
