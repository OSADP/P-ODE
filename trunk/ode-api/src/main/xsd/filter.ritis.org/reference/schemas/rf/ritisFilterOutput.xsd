<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<xs:schema xmlns="http://cattlab.umd.edu/schema/ritisFilter/other"
	xmlns:ai="http://jaxb.dev.java.net/plugin/if_insertion"
	xmlns:gml="http://www.opengis.net/gml/3.2"
	xmlns:gtfs="http://cattlab.umd.edu/schema/ritisFilter/gtfs"
	xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
	xmlns:rf="http://cattlab.umd.edu/schema/ritisFilter" xmlns:xs="http://www.w3.org/2001/XMLSchema"
	jaxb:version="2.0" targetNamespace="http://cattlab.umd.edu/schema/ritisFilter/other"
	version="1.1.1">

	<xs:import namespace="http://cattlab.umd.edu/schema/ritisFilter"
		schemaLocation="ritisFilterEventOutput.xsd"/>
	<xs:import namespace="http://cattlab.umd.edu/schema/ritisFilter/gtfs"
		schemaLocation="rf-gtfs.xsd"/>

	<xs:annotation>
		<xs:appinfo>
			<jaxb:schemaBindings>
				<jaxb:package name="edu.umd.cattlab.schema.ritisFilter.other"/>
			</jaxb:schemaBindings>
		</xs:appinfo>
	</xs:annotation>

	<xs:element name="tmcResponse" type="tmcResponse"/>
	<xs:element name="dmsDevices" type="dmsDevices"/>
	<xs:element name="detectorDevices" type="detectorDevices"/>
	<xs:element name="deviceData" type="device-data"/>
	<xs:element name="clarusData" type="clarus-data"/>
	<xs:element name="evacuationSupportData" type="evacuationSupportData"/>
	<xs:element name="vwsData" type="vwsData"/>
	<xs:element name="eventLaneCountResponse" type="eventLaneCountResponse"/>


	<xs:element name="vehicleData" type="vehicle-data"/>
	<xs:complexType name="vehicle-location">
		<xs:complexContent>
			<xs:extension base="rf:pointLocationType">
				<xs:sequence>
					<xs:element minOccurs="0" name="locationDetails">
						<xs:complexType>
							<xs:sequence>
								<xs:element minOccurs="0" name="heading" type="xs:decimal"/>
								<xs:element minOccurs="0" name="bearing" type="xs:decimal"/>
								<xs:element minOccurs="0" name="odometer"
									type="rf:nonNegativeDecimal"/>
								<xs:element minOccurs="0" name="speed" type="rf:nonNegativeDecimal"
								/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>

		</xs:complexContent>

	</xs:complexType>
	<xs:complexType name="vehicle-data">
		<xs:sequence>
			<xs:element name="header" type="rf:header"/>
			<xs:element name="vehicles">
				<xs:complexType>
					<xs:sequence>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="vehicle"
							type="vehicle"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="vehicle">
		<xs:sequence>
			<xs:element name="head">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="id" type="xs:normalizedString"/>
						<xs:element minOccurs="0" name="identifier" type="xs:normalizedString"/>
						<xs:element name="name" type="xs:string" minOccurs="0"/>
						<xs:element minOccurs="0" name="group" type="xs:normalizedString"/>
						<xs:element name="reportingAgency" type="xs:normalizedString"/>
						<xs:element name="reportingSystem" type="xs:normalizedString"/>
						<xs:element minOccurs="0" name="opCenter" type="xs:normalizedString"/>
						<xs:element name="type">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="standardType" type="vehicleType"/>
									<xs:element minOccurs="0" name="agencyType"
										type="xs:normalizedString"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="dates">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="updated" type="xs:dateTime"/>
									<xs:choice minOccurs="0">
										<xs:element minOccurs="0" name="online" type="xs:dateTime"/>
										<xs:element minOccurs="0" name="offline" type="xs:dateTime"
										/>
									</xs:choice>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element minOccurs="0" name="url" type="xs:anyURI"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element minOccurs="0" name="details" type="rf:vehicle"/>
			<xs:element name="location" type="vehicle-location"/>
			<xs:element minOccurs="0" name="passengers">
				<xs:complexType>
					<xs:sequence>
						<xs:element minOccurs="0" name="onBoard" type="xs:nonNegativeInteger">
							<xs:annotation>
								<xs:documentation>The number of passengers on board based on the more recent stop.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element minOccurs="0" name="boarded" type="xs:nonNegativeInteger">
							<xs:annotation>
								<xs:documentation>The number of passengers who boarded at the more recent stop.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element minOccurs="0" name="alighted" type="xs:nonNegativeInteger">
							<xs:annotation>
								<xs:documentation>The number of passengers who alighted at the more recent stop.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="negativeOneOrGreater">
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="-1"/>

		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="directionType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="north"/>
			<xs:enumeration value="south"/>
			<xs:enumeration value="east"/>
			<xs:enumeration value="west"/>
			<xs:enumeration value="northeast"/>
			<xs:enumeration value="northwest"/>
			<xs:enumeration value="southeast"/>
			<xs:enumeration value="southwest"/>
			<xs:enumeration value="reversible"/>
			<xs:enumeration value="inner loop"/>
			<xs:enumeration value="outer loop"/>
			<xs:enumeration value="both"/>
			<xs:enumeration value="unknown"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="vehicleType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="unknown"/>
			<xs:enumeration value="other"/>
			<xs:enumeration value="bus"/>
			<xs:enumeration value="subway train"/>
			<xs:enumeration value="train"/>
			<xs:enumeration value="taxi"/>
			<xs:enumeration value="ferry"/>
			<xs:enumeration value="cable car"/>
			<xs:enumeration value="suspended cable car"/>
			<xs:enumeration value="airplane"/>
			<xs:enumeration value="helicopter"/>
			<xs:enumeration value="emergency medical"/>
			<xs:enumeration value="fire and rescue"/>
			<xs:enumeration value="law enforcement"/>
			<xs:enumeration value="dot"/>
			<xs:enumeration value="tow truck"/>
			<xs:enumeration value="pickup truck"/>
			<xs:enumeration value="dump truck"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:complexType name="associatedEvents">
		<xs:sequence>
			<xs:element maxOccurs="unbounded" minOccurs="1" name="event" type="rf:association">
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="tmcResponse">
		<xs:sequence>
			<xs:element name="header" type="rf:header"/>
			<xs:element name="tmcs">
				<xs:complexType>
					<xs:sequence>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="tmc" type="tmc"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="tmc">
		<xs:sequence>
			<xs:element name="head">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="id" type="xs:string"/>
						<xs:element minOccurs="0" name="type" type="xs:string"/>
						<xs:element minOccurs="0" name="version" type="xs:integer"/>
						<xs:element name="roadOrder" type="xs:integer"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element minOccurs="0" name="location" type="rf:location"/>
			<xs:element minOccurs="0" name="length" type="rf:valueUnitType"/>
			<xs:element minOccurs="0" name="measurements">
				<xs:complexType>
					<xs:sequence>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="measurement">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="timestamp" type="xs:dateTime"/>
									<xs:element name="speed" type="xs:nonNegativeInteger"/>
									<xs:element name="averageSpeed" type="xs:nonNegativeInteger"/>
									<xs:element name="referenceSpeed" type="xs:nonNegativeInteger"/>
									<xs:element name="confidenceScore" type="xs:nonNegativeInteger"/>
									<xs:element name="travelTime" type="rf:valueUnitType"/>
									<xs:element name="freeFlowTravelTime" type="rf:valueUnitType"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>


	<xs:complexType name="dmsDevices">
		<xs:sequence>
			<xs:element name="header" type="rf:header"/>
			<xs:element name="devices">
				<xs:complexType>
					<xs:sequence>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="dmsDevice"
							type="dmsDevice"> </xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="dmsDevice">
		<xs:sequence>
			<xs:element name="head">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="id" type="xs:string"/>
						<xs:element minOccurs="0" name="name" type="xs:string"/>
						<xs:element name="reportingAgency" type="xs:normalizedString"/>
						<xs:element name="reportingSystem" type="xs:normalizedString"/>
						<xs:element name="type" type="dmsType"/>
						<xs:element minOccurs="0" name="lastCommTime" type="xs:dateTime"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="status" type="dmsStatus"/>
			<xs:element name="linkDirection" type="xs:nonNegativeInteger"/>
			<xs:element name="location" type="rf:location"/>
			<xs:element maxOccurs="1" minOccurs="0" name="messages" type="messages"/>
			<xs:element maxOccurs="1" minOccurs="0" name="associatedEvents" type="associatedEvents"
			/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="messages">
		<xs:sequence>
			<xs:element maxOccurs="unbounded" name="dmsMessage" type="dmsMessage"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="dmsMessage">

		<xs:sequence>
			<xs:element name="start-time" type="xs:dateTime"/>
			<xs:element minOccurs="0" name="end-time" type="xs:dateTime"/>
			<xs:element name="message" type="xs:string"/>
			<xs:element name="beacons" type="xs:int"/>
			<xs:element minOccurs="0" name="event-id" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>

	<xs:simpleType name="dmsType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="unknown"/>
			<xs:enumeration value="non-specified non-portable sign"/>
			<xs:enumeration value="black out sign"/>
			<xs:enumeration value="changeable message sign"/>
			<xs:enumeration value="variable message sign with character matrix setup"/>
			<xs:enumeration value="variable message sign with line matrix setup"/>
			<xs:enumeration value="variable message sign with full matrix setup"/>
			<xs:enumeration value="non-specified portable sign"/>
			<xs:enumeration value="portable black out sign"/>
			<xs:enumeration value="portable changeable message sign"/>
			<xs:enumeration value="portable variable message sign with character matrix setup"/>
			<xs:enumeration value="portable variable message sign with line matrix setup"/>
			<xs:enumeration value="portable variable message sign with full matrix setup"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="beaconType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="on"/>
			<xs:enumeration value="off"/>
			<xs:enumeration value="unknown"/>
		</xs:restriction>
	</xs:simpleType>


	<xs:simpleType name="dmsStatus">
		<xs:restriction base="xs:string">
			<xs:enumeration value="on"/>
			<xs:enumeration value="off"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:complexType name="detectorDevices">
		<xs:sequence>
			<xs:element name="header" type="rf:header"/>
			<xs:element name="devices">
				<xs:complexType>
					<xs:sequence>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="detectorDevice"
							type="detectorDevice"> </xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="detectorDevice">
		<xs:sequence>
			<xs:element name="head">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="id" type="xs:string"/>
						<xs:element name="name" type="xs:string"/>
						<xs:element name="reportingAgency" type="xs:normalizedString"/>
						<xs:element name="rtmc" type="xs:normalizedString"/>
						<xs:element name="region" type="xs:normalizedString"/>
						<xs:element name="type" type="detectorType"/>
						<xs:element minOccurs="0" name="network" type="xs:normalizedString"/>
						<xs:element minOccurs="0" name="pollingInterval" type="xs:positiveInteger"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="location" type="rf:location"/>
			<xs:element minOccurs="0" name="zones" type="zones"/>
			<xs:element maxOccurs="1" minOccurs="0" name="lanes" type="lanes"/>
			<xs:element name="offsetAngle" type="xs:double"/>
			<xs:element minOccurs="0" name="links">
				<xs:complexType>
					<xs:sequence>
						<xs:element maxOccurs="unbounded" name="link" type="link"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="link">
		<xs:sequence>
			<xs:element name="id" type="xs:normalizedString"/>
			<xs:element name="linkIdentifier" type="xs:normalizedString"/>
			<xs:element name="type" type="linkTypeRestriction"/>
			<xs:element name="length" type="rf:valueUnitType"/>
			<xs:element name="location" type="rf:location"/>
			<xs:element name="linkData">
				<xs:complexType>
					<xs:sequence>
						<xs:element maxOccurs="unbounded" name="linkDataItem">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="direction" type="rf:directionType"/>
									<xs:element name="detectionMethod"
										type="detectionMethodRestriction"/>
									<xs:element name="dataType" type="dataTypeRestriction"/>
									<xs:element name="status" type="linkStatusRestriction"/>
									<xs:element minOccurs="0" name="delay" type="xs:int"/>
									<xs:element name="travelTime" type="xs:int"/>
									<xs:element name="travelTimeIncrease" type="xs:int"/>
									<xs:element name="averageSpeed" type="xs:int"/>
									<xs:element minOccurs="0" name="currentDelay" type="xs:int"/>
									<xs:element minOccurs="0" name="predictedDelay" type="xs:int"/>
								</xs:sequence>
								<xs:attribute name="timestamp"/>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="zones">
		<xs:sequence>
			<xs:element maxOccurs="unbounded" minOccurs="1" name="zone" type="zone"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="zone">
		<xs:sequence>
			<xs:element name="zoneNumber" type="xs:nonNegativeInteger"/>
			<xs:element minOccurs="0" name="detectorIdentifier" type="xs:string"/>
			<xs:element minOccurs="0" name="numLanes" type="negativeOneOrGreater"/>
			<xs:element minOccurs="0" name="referenceSpeed" type="xs:nonNegativeInteger"/>
			<xs:element name="zoneLocation" type="rf:location"/>

			<xs:element name="zoneData">
				<xs:complexType>
					<xs:sequence>
						<xs:element maxOccurs="unbounded" minOccurs="1" name="zoneDataItem"
							type="zoneDataItem"> </xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element minOccurs="0" name="type" type="xs:normalizedString"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="zoneDataItem">
		<xs:sequence>
			<xs:element minOccurs="0" name="measurementStart" type="xs:dateTime"/>
			<xs:element minOccurs="0" name="measurementEnd" type="xs:dateTime"/>
			<xs:element minOccurs="0" name="measurementDuration" type="xs:positiveInteger"/>
			<xs:element minOccurs="0" name="effectiveDate" type="xs:dateTime"/>
			<xs:element minOccurs="0" name="expirationDate" type="xs:dateTime"/>
			<xs:element name="vehicleCount" type="xs:nonNegativeInteger"/>
			<xs:element name="speed" type="xs:nonNegativeInteger"/>
			<xs:element name="occupancy" type="negativeOneOrGreater"/>
			<xs:element name="quality" type="xs:nonNegativeInteger"/>
			<xs:element name="status" type="zoneStatus"/>
			<xs:element name="bearing" type="xs:nonNegativeInteger"/>
		</xs:sequence>
	</xs:complexType>

	<xs:simpleType name="detectorType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="unknown"/>
			<xs:enumeration value="other"/>
			<xs:enumeration value="rtms"/>
			<xs:enumeration value="acoustic"/>
			<xs:enumeration value="single loop"/>
			<xs:enumeration value="trap"/>
			<xs:enumeration value="170e"/>
			<xs:enumeration value="det"/>
			<xs:enumeration value="det t"/>
			<xs:enumeration value="piezo"/>
			<xs:enumeration value="piezo p"/>
			<xs:enumeration value="magnetometer (sensys)"/>
			<xs:enumeration value="microwave"/>
			<xs:enumeration value="atr"/>
			<xs:enumeration value="radar"/>
			<xs:enumeration value="lpr"/>
			<xs:enumeration value="avi"/>
			<xs:enumeration value="obe"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="zoneStatus">
		<xs:annotation>
			<xs:documentation>valid - data is valid and usable; invalid -
				data is invalid and unusable; incomplete - data is incomplete and
				partially usable; unknown - status is unknown; idle - detector's
				last measurement was far in the past</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="valid"/>
			<xs:enumeration value="invalid"/>
			<xs:enumeration value="incomplete"/>
			<xs:enumeration value="unknown"/>
			<xs:enumeration value="idle"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="weatherType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="clear"/>
			<xs:enumeration value="cloudy"/>
			<xs:enumeration value="sunny"/>
			<xs:enumeration value="snow"/>
			<xs:enumeration value="rain"/>
			<xs:enumeration value="icy"/>
			<xs:enumeration value="fog"/>
			<xs:enumeration value="sleet"/>
			<xs:enumeration value="hail"/>
			<xs:enumeration value="other"/>
			<xs:enumeration value="heat wave"/>
			<xs:enumeration value="unknown"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="hazmatLevelType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="minor"/>
			<xs:enumeration value="major"/>
			<xs:enumeration value="unknown"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:complexType name="crossLocationType">
		<xs:sequence>
			<xs:element maxOccurs="1" minOccurs="0" name="startRoad" type="xs:string"/>
			<xs:element maxOccurs="1" minOccurs="0" name="startMileMarker" type="xs:double"/>
			<xs:element maxOccurs="1" minOccurs="0" name="startLatitude" type="xs:double"/>
			<xs:element maxOccurs="1" minOccurs="0" name="startLongitude" type="xs:double"/>
			<xs:element maxOccurs="1" minOccurs="0" name="startExit" type="xs:nonNegativeInteger"/>
			<xs:element maxOccurs="1" minOccurs="0" name="endRoad" type="xs:string"/>
			<xs:element maxOccurs="1" minOccurs="0" name="endMileMarker" type="xs:double"/>
			<xs:element maxOccurs="1" minOccurs="0" name="endLatitude" type="xs:double"/>

			<xs:element maxOccurs="1" minOccurs="0" name="endLongitude" type="xs:double"/>
			<xs:element maxOccurs="1" minOccurs="0" name="endExit" type="xs:nonNegativeInteger"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="laneData">
		<xs:sequence>
			<xs:element maxOccurs="unbounded" minOccurs="1" name="lanes" type="lanes"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="lanes">
		<xs:sequence>
			<xs:element maxOccurs="unbounded" minOccurs="1" name="lane" type="zoneDataItem">
			</xs:element>
		</xs:sequence>
		<xs:attribute name="direction" type="directionType"/>
		<xs:attribute name="detectorIdentifier" type="xs:string"/>
	</xs:complexType>

	<xs:complexType name="clarus-data">
		<xs:sequence>
			<xs:element maxOccurs="1" minOccurs="1" name="header" type="rf:header"/>
			<xs:element maxOccurs="1" minOccurs="1" name="sites">
				<xs:complexType>
					<xs:sequence>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="site" type="site"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="site">
		<xs:sequence>
			<xs:element name="head">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="id" type="xs:string"/>
						<xs:element name="name" type="xs:string"/>
						<xs:element name="state-site-id" type="xs:string"/>
						<xs:element minOccurs="0" name="state-system-id" type="xs:string"/>
						<xs:element minOccurs="0" name="climate-id" type="xs:int"/>
						<xs:element name="contributor-id" type="xs:int"/>
						<xs:element name="contributor" type="xs:string"/>
						<xs:element name="reportingAgency" type="xs:normalizedString"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="localtion" type="rf:location"/>
			<xs:element minOccurs="0" name="access-directions" type="xs:string"/>
			<xs:element minOccurs="0" name="representativeness" type="xs:string"/>
			<xs:element minOccurs="0" name="obstructions" type="xs:string"/>
			<xs:element minOccurs="0" name="landscape" type="xs:string"/>
			<xs:element minOccurs="0" name="access-controlled" type="xs:int"/>
			<xs:element minOccurs="0" name="terrain-slope" type="xs:double"/>
			<xs:element minOccurs="0" name="terrain-slope-direction" type="xs:string"/>
